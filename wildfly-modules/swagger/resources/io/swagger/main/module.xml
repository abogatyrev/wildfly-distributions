<?xml version="1.0" ?>
<module xmlns="urn:jboss:module:1.3" name="io.swagger">
    <properties>
        <property name="jboss.api" value="private"/>
    </properties>

    <resources>
        <resource-root path="swagger-core-1.5.7.jar"/>
        <resource-root path="swagger-jaxrs-1.5.7.jar"/>
        <resource-root path="swagger-jersey2-jaxrs-1.5.7.jar"/>
        <resource-root path="swagger-annotations-1.5.7.jar"/>
        <resource-root path="swagger-models-1.5.7.jar"/>
    </resources>

    <dependencies>
        <module name="org.reflections"/>
        <module name="org.apache.commons-lang3"/>

        <module name="com.fasterxml.jackson.core.jackson-core"/>
        <module name="com.fasterxml.jackson.core.jackson-annotations"/>
        <module name="com.fasterxml.jackson.core.jackson-databind"/>
        <module name="com.fasterxml.jackson.jaxrs.jackson-jaxrs-json-provider"/>
        <module name="com.fasterxml.jackson.dataformat"/>

        <module name="org.slf4j"/>
        <module name="javax.ws.rs.api"/>
        <module name="javax.xml.bind.api"/>
        <module name="javax.servlet.api"/>
        <module name="javax.validation.api"/>

        <module name="com.google.guava"/>
    </dependencies>
</module>